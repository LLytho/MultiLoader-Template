{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "https://github.com/${github_user}/${github_repo}",
    "sources": "https://github.com/${github_user}/${github_repo}",
    "issues": "https://github.com/${github_user}/${github_repo}/issues"
  },
  "license": "MIT",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.llytho.examplemod.ExampleMod"
    ]
  },
  "mixins": [
    "examplemod-common.mixins.json",
    "examplemod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_min_loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_min_version}",
    "java": ">=17"
  }
}
